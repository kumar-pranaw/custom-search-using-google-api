<Window x:Class="ScrapeLinkedinData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScrapeLinkedinData"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="38" Margin="248,164,0,0" Name="SearchBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="226" TextChanged="TextBox_TextChanged"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="323,227,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Search Data Based on Custom Search Engine" HorizontalAlignment="Left" Margin="235,90,0,0" VerticalAlignment="Top" Height="32" Width="314"/>

    </Grid>
</Window>
